{
   "name": "Binary Addition",
   "description": "When started on a tape continaing two binary numbers, on the right end of the second number, this machine will replace the numbers with their sum.",
   "max_state": 25,
   "symbols": "xyzabc01$@",
   "tape": "#1011101#1001111#",
   "position": 15,
   "rules": [
      [ 0, "#", "#", 10, "L" ],
      [ 0, "0", "#", 6, "L" ],
      [ 0, "1", "#", 1, "L" ],
      [ 1, "#", "#", 2, "L" ],
      [ 1, "*", "*", 1, "L" ],
      [ 2, "#", "y", 4, "R" ],
      [ 2, "0", "y", 4, "R" ],
      [ 2, "1", "x", 3, "L" ],
      [ 2, "*", "*", 2, "L" ],
      [ 3, "#", "1", 4, "R" ],
      [ 3, "0", "1", 4, "R" ],
      [ 3, "1", "0", 3, "L" ],
      [ 4, "#", "#", 5, "R" ],
      [ 4, "*", "*", 4, "R" ],
      [ 5, "#", "#", 0, "L" ],
      [ 5, "*", "*", 5, "R" ],
      [ 6, "#", "#", 7, "L" ],
      [ 6, "*", "*", 6, "L" ],
      [ 7, "#", "x", 4, "R" ],
      [ 7, "0", "x", 4, "R" ],
      [ 7, "1", "y", 4, "R" ],
      [ 7, "*", "*", 7, "L" ],
      [ 10, "x", "0", 10, "L" ],
      [ 10, "y", "1", 10, "L" ],
      [ 10, "*", "*", 11, "R" ],
      [ 11, "#", "#", "h", "L" ],
      [ 11, "*", "*", 11, "R" ]
   ]
}
