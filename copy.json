{
   "name": "Copy abc",
   "description": "When started on the left end of a string containing only a's, b's and c's, this machine will make a copy of the string, with the copy to the right of the original.",
   "max_state": 25,
   "symbols": "xyzabc01$@",
   "tape": "babccaba##################",
   "position": 0,
   "rules": [
      [ 0, "#", "#", "h", "R" ],
      [ 0, "a", "@", 1, "R" ],
      [ 0, "b", "@", 5, "R" ],
      [ 0, "c", "@", 9, "R" ],
      [ 1, "#", "#", 2, "R" ],
      [ 1, "*", "*", 1, "R" ],
      [ 2, "#", "a", 3, "L" ],
      [ 2, "*", "*", 2, "R" ],
      [ 3, "#", "#", 4, "L" ],
      [ 3, "*", "*", 3, "L" ],
      [ 4, "@", "a", 0, "R" ],
      [ 4, "*", "*", 4, "L" ],
      [ 5, "#", "#", 6, "R" ],
      [ 5, "*", "*", 5, "R" ],
      [ 6, "#", "b", 7, "L" ],
      [ 6, "*", "*", 6, "R" ],
      [ 7, "#", "#", 8, "L" ],
      [ 7, "*", "*", 7, "L" ],
      [ 8, "@", "b", 0, "R" ],
      [ 8, "*", "*", 8, "L" ],
      [ 9, "#", "#", 10, "R" ],
      [ 9, "*", "*", 9, "R" ],
      [ 10, "#", "c", 11, "L" ],
      [ 10, "*", "*", 10, "R" ],
      [ 11, "#", "#", 12, "L" ],
      [ 11, "*", "*", 11, "L" ],
      [ 12, "@", "c", 0, "R" ],
      [ 12, "*", "*", 12, "L" ]
   ]
}
